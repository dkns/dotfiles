{
  "coc.preferences.formatOnType": true,
  "coc.preferences.hoverTarget": "float",
  "prettier.eslintIntegration": true,
  "prettier.tslintIntegration": true,
  "eslint.filetypes": ["javascript", "javascriptreact", "javascript.jsx"],
  "eslint.autoFix": false,
  "eslint.autoFixOnSave": false,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": "> ",
  "diagnostic.displayByAle": true,
  "python.jediEnabled": false,
  "codeLens.enable": true,
  "suggest.noselect": true,
  "yaml.format.enable": true,
  "yaml.validate": true,
  "yaml.hover": true,
  "yaml.completion": true,
  "yaml.schemas": {
    "http://json.schemastore.org/gitlab-ci": ".gitlab-ci.yml",
    "http://json.schemastore.org/package": "package.json",
    "http://json.schemastore.org/eslintrc": ".eslintrc",
    "http://json.schemastore.org/prettierrc": ".prettierrc.json"
  },
  "languageserver": {
    "sh": {
      "command": "bash-language-server",
      "args": ["start", "--stdio"],
      "filetypes": ["sh"],
      "cwd": "./",
      "initializationOptions": {},
      "ignoredRootPaths": ["~"],
      "settings": {}
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["Dockerfile"],
      "args": ["--stdio"]
    }
  }
}
