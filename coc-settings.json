{
  "coc.preferences.formatOnType": true,
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.previewAutoClose": true,
  "prettier.eslintIntegration": true,
  "prettier.tslintIntegration": true,
  "eslint.filetypes": ["javascript", "javascriptreact", "javascript.jsx", "typescript"],
  "eslint.autoFix": true,
  "eslint.autoFixOnSave": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": ">> ",
  "diagnostic.displayByAle": false,
  "diagnostic.virtualTextLineSeparator": " \\ ",
  "python.jediEnabled": false,
  "codeLens.enable": true,
  "suggest.noselect": true,
  "list.nextKeymap": "<C-n>",
  "list.previousKeymap": "<C-p>",
  "list.previewSplitRight": true,
  "coc.preferences.rootPatterns": [
    ".vim",
    ".git",
    ".hg",
    ".projections.json",
    ".env"
  ],
  "docker.enable": true,
  "importCost.bundleSizeDecoration": "both",
  "importCost.showCalculatingDecoration": true,
  "importCost.javascriptExtensions": [
    "\\.js?$",
    "\\.jsx?$"
  ],
  "importCost.typescriptExtensions": [
    "\\.tsx?$"
  ],
  "git.enableGutters": true,
  "git.addGlameToVirtualText": true,
  "git.addedSign.text": "┃",
  "git.changedSign.text": "┃",
  "git.removedSign.text": "┃",
  "yaml.format.enable": true,
  "yaml.validate": true,
  "yaml.hover": true,
  "yaml.completion": true,
  "yaml.schemas": {
    "http://json.schemastore.org/gitlab-ci": ".gitlab-ci.yml",
    "http://json.schemastore.org/package": "package.json",
    "http://json.schemastore.org/eslintrc": ".eslintrc",
    "http://json.schemastore.org/prettierrc": ".prettierrc.json"
  },
  "diagnostic-languageserver.filetypes": {
    "sh": "shellcheck"
  },
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start", "--stdio"],
      "filetypes": ["sh", "zsh"],
      "cwd": "./",
      "initializationOptions": {},
      "ignoredRootPaths": ["~"],
      "settings": {}
    }
  }
}
